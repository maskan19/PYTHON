

import mysql.connector

mydb = mysql.connector.connect(
    host = "localhost",
    user = "root",
    password = "python",
    database = "python"
)

mycursor = mydb.cursor()

sql = "INSERT INTO sample (col1, col2, col3) VALUES (%s, %s %s)"

val = [
    ('CHOI', 'Lowstreet4', 'd'),
    ('YangCHOI', 'ulsan', 'd'),
    ('BAEK', 'busan', 'f'),
    ('PARK', 'seoul', 'g'),
    ('HAN', 'jinju', 'r')
]

mycursor.executemany(sql, val)

mydb.commit()

print(mycursor.rowcount, "record was inserted")