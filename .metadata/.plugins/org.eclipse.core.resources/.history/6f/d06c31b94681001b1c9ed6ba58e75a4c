class Dog: 

    def __init__(self):
        self.flag_bark = True

    def cutSungdate(self):
        self.flag_bark = False


class Bird: 

    def __init__(self):
        self.idx_fly = 0

    def training(self):
        self.idx_fly += 1

    def trainging_hard(self, power):
        self.idx_fly += power
