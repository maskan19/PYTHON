class Dog: 

    def __init__(self):
        self.flag_bark = True

    def cutSungdae(self):
        self.flag_bark = False


class Bird: 

    def __init__(self):
        self.idx_fly = 0

    def training(self):
        self.idx_fly += 1

    def trainging_hard(self, power):
        self.idx_fly += power

class GaeSae(Dog, Bird):
    
    def __init__(self):
        super().__init__()
        super().flag_bark()
        super().idx_fly()
        pass
    
if __name__ =='__main__' :
    gs = GaeSae()
    print(gs.flag_bark)
    print(gs.idx_fly)
    
    gs.cutSungdae()
    gs.trainging_hard(4)
    print(gs.flag_bark)
    print(gs.idx_fly)
    